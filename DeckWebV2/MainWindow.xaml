<Fluent:RibbonWindow  x:Class="DeckWebV2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:Fluent="urn:fluent-ribbon"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeckWebV2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Fluent:Ribbon>
            <Fluent:RibbonTabItem Header="Main" KeyTip="M">
                <Fluent:RibbonGroupBox Header="Main3" KeyTip="N">
                    <Fluent:Button Header="Close" KeyTip="I"></Fluent:Button>
                </Fluent:RibbonGroupBox>
            </Fluent:RibbonTabItem>
            <Fluent:RibbonTabItem Header="View" KeyTip="V">
                
            </Fluent:RibbonTabItem>
            
        </Fluent:Ribbon>
    </Grid>
</Fluent:RibbonWindow>
